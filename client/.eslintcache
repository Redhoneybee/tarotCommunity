ent || Images.length === 0) {\r\n            return alert(\" Î™®Îì† Í∞íÏùÑ ÎÑ£Ïñ¥Ï£ºÏÖîÏïº Ìï©ÎãàÎã§.\")\r\n        }\r\n\r\n\r\n        //ÏÑúÎ≤ÑÏóê Ï±ÑÏö¥ Í∞íÎì§ÏùÑ requestÎ°ú Î≥¥ÎÇ∏Îã§.\r\n\r\n        const body = {\r\n            //Î°úÍ∑∏Ïù∏ Îêú ÏÇ¨ÎûåÏùò ID \r\n            writer: props.user.userData._id,\r\n            title: Title,\r\n            description: Description,\r\n            images: Images,\r\n            continents: Continent\r\n        }\r\n\r\n        Axios.post('/api/product', body)\r\n            .then(response => {\r\n                if (response.data.success) {\r\n                    alert('ÏÉÅÌíà ÏóÖÎ°úÎìúÏóê ÏÑ±Í≥µ ÌñàÏäµÎãàÎã§.')\r\n                    props.history.push('/')\r\n                } else {\r\n                    alert('ÏÉÅÌíà ÏóÖÎ°úÎìúÏóê Ïã§Ìå® ÌñàÏäµÎãàÎã§.')\r\n                }\r\n            })\r\n    }\r\n\r\n\r\n    return (\r\n        <div style={{ maxWidth: '700px', margin: '2rem auto' }}>\r\n            <div style={{ textAlign: 'center', marginBottom: '2rem' }}>\r\n                <h2> Ïó¨Ìñâ ÏÉÅÌíà ÏóÖÎ°úÎìú</h2>\r\n            </div>\r\n\r\n            <Form onSubmit={submitHandler}>\r\n                {/* DropZone */}\r\n                <FileUpload refreshFunction={updateImages} />\r\n\r\n                <br />\r\n                <br />\r\n                <label>Ïù¥Î¶Ñ</label>\r\n                <Input onChange={titleChangeHandler} value={Title} />\r\n                <br />\r\n                <br />\r\n                <label>ÏÑ§Î™Ö</label>\r\n                <TextArea onChange={descriptionChangeHandler} value={Description} />\r\n                <br />\r\n                <br />\r\n                <select onChange={continentChangeHandler} value={Continent}>\r\n                    {Continents.map(item => (\r\n                        <option key={item.key} value={item.key}> {item.value}</option>\r\n                    ))}\r\n                </select>\r\n                <br />\r\n                <br />\r\n           